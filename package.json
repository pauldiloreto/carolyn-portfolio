{
  "name": "carolyn-portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 3000 --host",
    "build": "vite build",
    "preview": "vite preview --port 4000",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:e2e": "playwright test",
    "test:unit": "vitest"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/types": "^18.4.3",
    "@csstools/postcss-global-data": "^2.1.0",
    "@playwright/test": "^1.40.0",
    "@svelte-put/preprocess-inline-svg": "^2.1.3",
    "@sveltejs/adapter-netlify": "^2.0.8",
    "@sveltejs/kit": "^1.27.6",
    "@types/marked": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "browserslist": "^4.22.1",
    "contentful": "^10.6.12",
    "cssnano": "^6.0.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-svelte": "^2.35.1",
    "playwright": "^1.40.0",
    "postcss": "^8.4.31",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.2",
    "svelte-eslint-parser": "0.33.1",
    "svelte-preprocess": "^5.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vitest": "^0.34.6"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/karla": "^5.0.18",
    "@fontsource/old-standard-tt": "^5.0.17",
    "jimp": "^0.22.10",
    "marked": "10.0.0",
    "p-all": "^5.0.0",
    "svelte-bricks": "^0.2.1"
  },
  "packageManager": "yarn@3.6.3"
