{
	"name": "carolyn-portfolio",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 3000 --host",
		"build": "vite build",
		"preview": "vite preview --port 4000",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"@commitlint/cli": "^17.7.1",
		"@commitlint/config-conventional": "^17.7.0",
		"@commitlint/types": "^17.4.4",
		"@csstools/postcss-global-data": "^2.1.0",
		"@playwright/test": "^1.38.1",
		"@svelte-put/preprocess-inline-svg": "^2.1.3",
		"@sveltejs/adapter-netlify": "^2.0.8",
		"@sveltejs/kit": "^1.25.1",
		"@types/marked": "^5.0.1",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"browserslist": "^4.21.11",
		"contentful": "^10.5.2",
		"cssnano": "^6.0.1",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-svelte": "^2.33.2",
		"postcss": "^8.4.30",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-normalize": "^10.0.1",
		"postcss-preset-env": "^9.1.4",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"svelte-eslint-parser": "0.33.0",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vitest": "^0.34.5"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/karla": "^5.0.12",
		"@fontsource/old-standard-tt": "^5.0.12",
		"jimp": "^0.22.10",
		"marked": "9.0.3",
		"p-all": "^5.0.0",
		"svelte-bricks": "^0.2.1"
	},
	"packageManager": "yarn@3.6.3"
}
